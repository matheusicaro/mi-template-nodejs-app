  "paths": {
    "/helth": {
      "post": {
        "tags": ["helth"],
        "summary": "check the Api status",
        "description": "",
        "operationId": "getHealth",
        "produces": ["application/json"],
        "parameters": [],
        "responses": {
          "200": { "status": "ONLINE", "schema": { "$ref": "#/definitions/HealthStatus" } }
        }
      }
    }
  },

  "definitions": {
    "HealthStatus": {
      "type": "object",
      "properties": {
        "time": { "type": "string", "format": "date-time", "example": "2021-01-30T08:30:00Z" },
        "status": { "type": "string", "enum": ["ONLINE", "FAILED"] }
      }
    }
  },
